<p style="color: green"><%= notice %></p>

<h1>Clients</h1>

<div id="clients">
  <% @clients.each do |client| %>
    <%= render client %>
    <p>
      <%= link_to "Show this client", client %>
    </p>
    <p>
      <%= link_to "Add some address", new_address %>
    </p>
  <% end %>
</div>

<%= link_to "New client", new_client_path %>
